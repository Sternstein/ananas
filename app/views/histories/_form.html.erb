<%= simple_form_for(@history) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :history %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

<hr/>
<%= render partial: 'md_helper' %>
<hr/>
<%= react_component 'PictureUploader' %>
<%= react_component 'MdPreview', element_id: '#history_history' %>
