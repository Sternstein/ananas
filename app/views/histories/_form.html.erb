<%= simple_form_for(@history) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :email %>
    <%= f.input :history %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
  <%= react_component 'PictureUploader' %>
  <%= react_component 'MdPreview', element_id: '#history_history' %>
